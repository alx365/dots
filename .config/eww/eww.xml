<eww>
  <definitions>
    <def
        name="list">
     <literal content="{{list}}"/> 
    </def>
    <def name="combo-box">
      <literal content="{{games}}"/>
    </def>
  </definitions>
  <variables>
    <script-var name="list" interval="1s">
      egs.sh
    </script-var>
    <script-var name="games" interval="100000000000000s">
      games.sh item
    </script-var>
  </variables>
  
  <windows>
    <window name="egs">
      <size x="250" y="80" />
      <pos x="0" y="0" />
      <widget>
        <box orientation="v">
          Games
          <list/>
          <combo-box/>
          <button onclick="/home/legend/programming/eww/target/debug/eww close {{window_name}}">Quit</button>
          </box>
      </widget>
    </window>
  </windows>
</eww>
